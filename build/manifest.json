{
	"manifest_version": 2,
	"name": "Focus App",
	"description": "An extension to help you focus",
	"version": "1.0",
	"icons": {
		"16": "images/icons/focus-app16.png",
		"48": "images/icons/focus-app48.png",
		"128": "images/icons/focus-app128.png"
	},
	"browser_action": {
		"default_icon": "images/icons/focus-app-red38.png",
		"default_popup": "html/popup.html"
	},
	"permissions": [
		"storage"
	],
	"background": {
		"page": "html/background.html"
	},
	"web_accessible_resources": [
		"images/icons/focus-app-38.png"
	]
}